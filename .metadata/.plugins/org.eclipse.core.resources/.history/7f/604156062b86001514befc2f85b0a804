package com.example.rss01;

import java.io.Serializable;

public class WeatherDetails implements Serializable {
	Long DayTemp;
	Long MinTemp;
	Long MaxTemp;
	Long NightTemp;
	Long EveTemp;
	Long MornTemp;
	Long Pressure;
	Long Humidity;
	String MainWeather;
	String DescriptionWeather;
	Long Cloudiness;
	@Override
	public String toString() {

		return String.format("%s   %s  DayTemp: %d, MinTemp: %d  %d",MainWeather,DescriptionWeather, DayTemp,MinTemp,Pressure);
	}

}
